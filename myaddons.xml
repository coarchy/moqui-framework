<addons default-repository="github-ssh">

    <!-- coarchy Client Components -->
    <component name="coarchy-com" group="coarchy" version="" branch="coarchy"/>
    <component name="coarchy" group="coarchy" version="" branch="coarchy"/>
    <runtime name="moqui-runtime" group="coarchy" version="" branch="coarchy"/>
    <runtime name="mantle-usl" group="moqui" version="" branch="decimalDigits"/>

    <!-- Moqui Componenets -->
    <component name="stripe" group="coarchy" version="" branch="master"/>
    <component name="MarbleERP" group="moqui" version="1.0.0" branch="master"/>

    <!-- acetousk Components -->
    <component group="acetousk" name="sentry" branch="master"/>

    <!-- Component Sets -->
    <component-set name="coarchy" components="MarbleERP,coarchy,WorkManagement,sentry"/>

    <!-- Release builds:
        gradle getComponentSet -PcomponentSet=custom -PlocationType=release
        - these make the source distro for each
        - to build the demo war: gradle load test addRuntime
    -->
</addons>
